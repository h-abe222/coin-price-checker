{
  "name": "coin-price-checker",
  "version": "1.0.0",
  "type": "module",
  "description": "BullionStar price monitoring system with Cloudflare Workers",
  "scripts": {
    "check-prices": "node cloudflare/price-checker.js",
    "compare-prices": "node cloudflare/multi-site-price-checker.js",
    "compare-product": "node cloudflare/multi-site-price-checker.js compare",
    "update-parallel": "node cloudflare/multi-site-parallel-updater.js",
    "update-product": "node cloudflare/multi-site-parallel-updater.js update",
    "update-prices": "node cloudflare/dynamic-price-updater.js",
    "update-price": "node cloudflare/dynamic-price-updater.js update",
    "list-sites": "node cloudflare/multi-site-price-checker.js sites",
    "migrate-db": "cd cloudflare && node migrate_schema.js",
    "migrate-db-remote": "cd cloudflare && node migrate_schema.js --remote",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "playwright": "^1.40.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  }
}